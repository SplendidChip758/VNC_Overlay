{
    "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/main/src/NerdBank.GitVersioning/version.schema.json",
    "version": "1.0-beta",
    "publicReleaseRefSpec": [
        "^refs/heads/main$",
        "^refs/heads/v\\d+(?:\\.\\d+)?$"
    ],
    "cloudBuild": {
        "buildNumber": {
            "enabled": true
        }
    },
    "release": {
        "branchName": "main",
        "versionHeightOffset": 0
    },
    "prereleaseBranches": {
        "dev": {
            "name": "alpha",
            "increment": "build"
        },
        "*": {
            "name": "beta",
            "increment": "build"
        }
    }
}